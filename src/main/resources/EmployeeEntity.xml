<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="model.Employee">
        <table name="employee" schema="public" catalog="skypro"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="firstName">
                <column name="first_name" length="50"/>
            </basic>
            <basic name="lastName">
                <column name="last_name" length="50"/>
            </basic>
            <basic name="gender">
                <column name="gender" length="6"/>
            </basic>
            <basic name="age">
                <column name="age" precision="-1"/>
            </basic>
            <basic name="cityId">
                <column name="city_id" nullable="false" precision="-1"/>
            </basic>
            <!--<many-to-one name="cityByCityId" optional="true" target-entity="model.City">
                <join-column name="city_id" nullable="false" referenced-column-name="city_id"/>
            </many-to-one>-->
        </attributes>
    </entity>
</entity-mappings>
